general_settings:
  telemetry: false
  master_key: sk-128d6e9dad2c1b6e592e16b650f7f822caaeaa830a8214f60d78fdf5e9b2f087

proxy_settings:
  host: 127.0.0.1
  port: 4000
  cors:
    - "*"

litellm_settings:
  set_verbose: false
  drop_params: true
  timeout: 120

model_list:
  # Primary alias → real OpenAI model
  - model_name: gpt-5
    litellm_params:
      model: openai/gpt-5
      api_key: os.environ/OPENAI_API_KEY
      timeout: 120

  # "Take over" common Claude names by pointing them to GPT-5 too
  - model_name: sonnet
    litellm_params:
      model: openai/gpt-5
      api_key: os.environ/OPENAI_API_KEY

  # fast path
  - model_name: haiku
    litellm_params:
      model: openai/gpt-5-mini
      api_key: os.environ/OPENAI_API_KEY

# NOTE: We do NOT set allowed_routes here → all routes exposed by default (dev-friendly)
